{
    "name": "Web Footprint",
    "description": "Finds your total network use and carbon footprint from the web!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "devtools_page": "devtools.html",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Open the popup",
        "default_icon": {
          "16": "/images/green16.png",
          "32": "/images/green32.png",
          "48": "/images/green48.png",
          "128": "/images/green128.png"
        }
      },
      "icons": {
        "16": "/images/green16.png",
        "32": "/images/green32.png",
        "48": "/images/green48.png",
        "128": "/images/green128.png"
      }
}